{
    "name": "sarahsolus/contaobsdesigner",
    "description": "Responsive Bootstrap Design Functionality for Contao 4 Backend",
    "keywords":["contao", "bootstrap", "theme"],
    "license": "LGPL-3.0-or-later",
    "type": "contao-bundle",
    "authors": [
        {
            "name": "Thomas Richter"
        }
    ],
    "support": {
        "issues": "https://github.com/Sarahsolus/contao-bootstrap-designer-plugin/issues/",
        "source": "https://github.com/Sarahsolus/contao-bootstrap-designer-plugin"
    },
    "require": {
        "php": ">=7.4",
        "contao/core-bundle": "^4.9 || ^4.13",
        "menatwork/contao-multicolumnwizard-bundle": "*"
    },
    "require-dev": {
    },
    "conflict": {
    },
    "suggest": {
        "contao-bootstrap/bundle": "Adds Bootstrap functionality to Contao"
    },
    "autoload": {
        "psr-4": {
            "Sarahsolus\\ContaoBsDesigner\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao/manager-plugin": true
        }
    },
    "extra": {
        "contao-manager-plugin": "Sarahsolus\\ContaoBsDesigner\\ContaoManager\\Plugin"
    }
}
